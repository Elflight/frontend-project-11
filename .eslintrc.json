{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["airbnb-base"],
    "plugins": ["import"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-extra-semi": "off",
      "import/extensions": ["error", "ignorePackages", {
        "js": "never",
        "mjs": "never"
      }]
    }
  }